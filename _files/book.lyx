\end_layout

\begin_layout Itemize
Implications: search high dimensional data by assuming it is implicitly
 lower dimensional.
 
\end_layout

\begin_layout Standard
\align center
\begin_inset Formula \[
p\left(\dataMatrix|\mappingMatrix\right)=\prod_{i=1}^{\numData}\gaussianDist{\dataVector_{i,:}}{\zerosVector}{\mappingMatrix\mappingMatrix^{\mathrm{T}}+\dataStd^{2}\eye}\]

\end_inset


\begin_inset Formula \[
p\left(\dataMatrix|\mappingMatrix\right)=\prod_{i=1}^{\numData}\gaussianDist{\dataVector_{i,:}}{\zerosVector}{\covarianceMatrix},\,\,\,\,\,\,\,\covarianceMatrix=\mappingMatrix\mappingMatrix^{\mathrm{T}}+\dataStd^{2}\eye\]

\end_inset


\begin_inset Formula \[
\log p\left(\dataMatrix|\mappingMatrix\right)=-\frac{\numData}{2}\log\left|\covarianceMatrix\right|-\frac{1}{2}\mbox{tr}\left(\covarianceMatrix^{-1}\dataMatrix^{\mathrm{T}}\dataMatrix\right)+\mbox{const.}\]

\end_inset


\end_layout

\begin_layout Standard

\series bold
Gradient of log likelihood
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\frac{\mathrm{d}}{\mathrm{d}\mappingMatrix}\log p\left(\dataMatrix|\mappingMatrix\right)=-\frac{N}{2}\covarianceMatrix^{-1}\mappingMatrix+\frac{1}{2}\covarianceMatrix^{-1}\dataMatrix^{\mathrm{T}}\dataMatrix\covarianceMatrix^{-1}\mappingMatrix\]

\end_inset

Seek fixed points
\begin_inset Formula \[
\zerosVector=-\frac{N}{2}\covarianceMatrix^{-1}\mappingMatrix+\frac{1}{2}\covarianceMatrix^{-1}\dataMatrix^{\mathrm{T}}\dataMatrix\covarianceMatrix^{-1}\mappingMatrix\]

\end_inset

pre-multiply by 2
\begin_inset Formula $\covarianceMatrix$
\end_inset


\begin_inset Formula \[
\zerosVector=-N\mappingMatrix+\dataMatrix^{\mathrm{T}}\dataMatrix\covarianceMatrix^{-1}\mappingMatrix\]

\end_inset


\begin_inset Formula \[
\frac{1}{\numData}\dataMatrix^{\mathrm{T}}\dataMatrix\covarianceMatrix^{-1}\mappingMatrix=\mappingMatrix\]

\end_inset


\begin_inset Newpage newpage
\end_inset


\end_layout

\begin_layout Standard

\series bold
Substitute 
\begin_inset Formula $\mappingMatrix$
\end_inset

 with singular value decomposition
\series default

\begin_inset Formula \[
\mappingMatrix=\eigenvectorMatrix\mathbf{L}\rotationMatrix^{\mathrm{T}}\]

\end_inset

which implies
\begin_inset Formula \begin{align*}
\covarianceMatrix & =\mappingMatrix\mappingMatrix^{\mathrm{T}}+\dataStd^{2}\eye\\
 & =\eigenvectorMatrix\mathbf{L}^{2}\eigenvectorMatrix^{\mathrm{T}}+\dataStd^{2}\mathbf{I}\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Using matrix inversion lemma
\begin_inset Note Note
status collapsed

\begin_layout Plain Layout
\begin_inset Formula \[
\covarianceMatrix^{-1}=\dataStd^{-2}\mathbf{\eye}-\dataStd^{-4}\eigenvectorMatrix\mathbf{L}\left(\eye+\mathbf{L}^{2}\right)^{-1}\mathbf{L}\eigenvectorMatrix^{\mathrm{T}}\]

\end_inset


\begin_inset Formula \[
\covarianceMatrix^{-1}=\dataStd^{-2}\mathbf{\eye}-\dataStd^{-4}\eigenvectorMatrix\mathbf{L}\left(\eye+\dataStd^{-2}\mathbf{L}^{2}\right)^{-1}\mathbf{L}\eigenvectorMatrix^{\mathrm{T}}\]

\end_inset


\begin_inset Formula \[
\covarianceMatrix^{-1}\mappingMatrix=\dataStd^{-2}\eigenvectorMatrix\mathbf{L}\rotationMatrix^{\mathrm{T}}-\dataStd^{-4}\eigenvectorMatrix\mathbf{L}\left(\eye+\dataStd^{-2}\mathbf{L}^{2}\right)^{-1}\mathbf{L}^{2}\rotationMatrix^{\mathrm{T}}\]

\end_inset


\begin_inset Formula \[
\covarianceMatrix^{-1}\mappingMatrix=\eigenvectorMatrix\mathbf{L}\left(\dataStd^{-2}\eye-\dataStd^{-4}\left(\eye+\dataStd^{-2}\mathbf{L}^{2}\right)^{-1}\mathbf{L}^{2}\right)\rotationMatrix^{\mathrm{T}}\]

\end_inset


\end_layout

\end_inset


\begin_inset Formula \[
\covarianceMatrix^{-1}\mappingMatrix=\eigenvectorMatrix\mathbf{L}\left(\dataStd^{2}+\mathbf{L}^{2}\right)^{-1}\rotationMatrix^{\mathrm{T}}\]

\end_inset


\begin_inset Note Note
status open

\begin_layout Plain Layout
\begin_inset Formula \[
\frac{1}{\numData}\mathbf{Y}^{\mathrm{T}}\mathbf{Y}\eigenvectorMatrix\mathbf{L}\left(\dataStd^{2}+\mathbf{L}^{2}\right)^{-1}\rotationMatrix^{\mathrm{T}}=\eigenvectorMatrix\mathbf{L}\rotationMatrix^{\mathrm{T}}\]

\end_inset

post-multiply by 
\begin_inset Formula $\mathbf{R}\mathbf{L}^{-1}$
\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard

\series bold
\begin_inset Newpage newpage
\end_inset

Solution given by
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\frac{1}{\numData}\mathbf{Y}^{\mathrm{T}}\mathbf{Y}\eigenvectorMatrix=\eigenvectorMatrix\left(\dataStd^{2}+\mathbf{L}^{2}\right)\]

\end_inset

which is recognised as an eigenvalue problem.
 
\end_layout

\begin_layout Itemize
This implies that the columns of 
\begin_inset Formula $\eigenvectorMatrix$
\end_inset

are the eigenvectors of 
\begin_inset Formula $\frac{1}{\numData}\dataMatrix^{\mathrm{T}}\dataMatrix$
\end_inset

 and that 
\begin_inset Formula $\dataStd^{2}+\mathbf{L}^{2}$
\end_inset

are the eigenvalues of 
\begin_inset Formula $\frac{1}{\numData}\dataMatrix^{\mathrm{T}}\dataMatrix$
\end_inset

.
 So 
\begin_inset Formula $l_{i}=\sqrt{\lambda_{i}-\dataStd^{2}}$
\end_inset

 where 
\begin_inset Formula $\lambda_{i}$
\end_inset

 is the 
\begin_inset Formula $i$
\end_inset

th eigenvalue of 
\begin_inset Formula $\frac{1}{\numData}\dataMatrix^{\mathrm{T}}\dataMatrix$
\end_inset

.
 
\end_layout

\begin_layout Itemize
Further manipulation shows that if we constrain 
\begin_inset Formula $\mappingMatrix\in\Re^{\dataDim\times\latentDim}$
\end_inset

 then the solution is given by the largest 
\begin_inset Formula $\latentDim$
\end_inset

 eigenvalues.
\end_layout

\begin_layout Standard
Probabilistic PCA Solution
\end_layout

\begin_layout Itemize
If 
\begin_inset Formula $\eigenvectorMatrix_{\latentDim}$
\end_inset

 are first 
\begin_inset Formula $\latentDim$
\end_inset

 principal eigenvectors of 
\begin_inset Formula $\numData^{-1}\dataMatrix^{\mathrm{T}}\dataMatrix$
\end_inset

 and the corresponding eigenvalues are 
\begin_inset Formula $\Lambda_{\latentDim}$
\end_inset

,
\begin_inset Formula \[
\mappingMatrix=\eigenvectorMatrix_{\latentDim}\mathbf{L}\rotationMatrix^{\mathrm{T}},\,\,\,\,\,\,\,\mathbf{L}=\left(\Lambda_{\latentDim}-\dataStd^{2}\eye\right)^{\frac{1}{2}}\]

\end_inset

where 
\begin_inset Formula $\rotationMatrix$
\end_inset

 is an arbitrary rotation matrix.
\end_layout

