\begin_inset Formula \[
q\left(\switchVector_{i,:}\right)=p\left(\switchVector_{i,:}|\dataVector_{i,:},\paramVector\right)\]

\end_inset

 and maximizing 
\begin_inset Formula \[
\sum_{i=1}^{\numData}\left\langle \log p\left(\dataVector_{i,:},\switchVector_{i,:}|\paramVector\right)\right\rangle _{q\left(\switchVector_{i,:}\right)}\]

\end_inset

