\end_inset


\begin_inset Formula \[
\sum_{k=1}^{\numComponents}\left\langle s_{i,k}\right\rangle \log\gaussianDist{\dataScalar_{i,j}}{f_{j}\left(\hat{\mathbf{x}}_{k,:};\mappingMatrix,l\right)}{\dataStd^{2}}=\frac{\dataDim}{2}\log\left(2\pi\dataStd^{2}\right)-\sum_{k=1}^{\numComponents}\left\langle \switchScalar_{i,k}\right\rangle \frac{\left(\dataVector_{i,:}-\mathbf{f}_{k,:}\right)^{\mathrm{T}}\left(\dataVector_{i,:}-\mathbf{f}_{k,:}\right)}{2\dataStd^{2}}\]

\end_inset


\begin_inset Formula \[
\sum_{k=1}^{\numComponents}\left\langle s_{i,k}\right\rangle \log\gaussianDist{\dataScalar_{i,j}}{f_{j}\left(\hat{\mathbf{x}}_{k,:};\mappingMatrix,l\right)}{\dataStd^{2}}=\frac{\dataDim}{2}\log\left(2\pi\dataStd^{2}\right)-\dataVector_{i,:}^{\mathrm{T}}\dataVector_{i,:}+\left\langle \switchScalar_{i,k}\right\rangle \dataVector_{i,:}^{\mathrm{T}}\mathbf{f}_{k,:}-\left\langle \switchScalar_{i,k}\right\rangle \mathbf{f}_{k,:}^{\mathrm{T}}\mathbf{f}_{k,:}\frac{}{}\sum_{k=1}^{\numComponents}\frac{\left(\dataVector_{i,:}-\mathbf{f}_{k,:}\right)^{\mathrm{T}}\left(\dataVector_{i,:}-\mathbf{f}_{k,:}\right)}{2\dataStd^{2}}\]

\end_inset


\begin_inset Formula \[
\frac{\mathrm{d}}{\mathrm{d}\mappingMatrix}\left\langle \log p\left(\dataVector_{i,:},\switchVector_{i,:}|\paramVector\right)\right\rangle _{q\left(\switchVector_{i,:}\right)}=\sum_{i=1}^{\numData}\sum_{j=1}^{\dataDim}\sum_{k=1}^{\numComponents}\left\langle s_{i,k}\right\rangle \frac{\left(y_{i,j}-f_{j}\left(\hat{\mathbf{x}}_{k,:};\mappingMatrix,l\right)\right)}{\dataStd^{2}}\frac{\mathrm{d}f_{j}\left(\hat{\mathbf{x}}_{k,:};\mappingMatrix,l\right)}{\mathrm{d}\mappingMatrix}\]

\end_inset


\begin_inset Formula \[
\frac{\mathrm{d}}{\mathrm{d}\mappingMatrix}\left\langle \log p\left(\dataVector_{i,:},\switchVector_{i,:}|\paramVector\right)\right\rangle _{q\left(\switchVector_{i,:}\right)}=\sum_{i=1}^{\numData}\sum_{j=1}^{\dataDim}\sum_{k=1}^{\numComponents}\left\langle s_{i,k}\right\rangle \frac{\left(\dataMatrix-\Phi\mappingMatrix\right)}{\dataStd^{2}}\frac{\mathrm{d}f_{j}\left(\hat{\mathbf{x}}_{k,:};\mappingMatrix,l\right)}{\mathrm{d}\mappingMatrix}\]

\end_inset


\begin_inset Formula \[
\left(\dataMatrix-\right)\Phi\]

\end_inset


\end_layout

\begin_layout Itemize
Marginal likelihood is now tractable
\begin_inset Formula \[
p\left(\dataMatrix|\paramVector\right)=\prod_{i=1}p\left(\dataVector_{i,:}|\paramVector\right)\]

\end_inset


\begin_inset Formula \[
p\left(\dataVector|\paramVector\right)=\int p\left(\dataVector_{i,:}|\latentVector_{i,:},\paramVector\right)\sum_{\switchVector}p\left(\latentVector_{i,:}|\switchVector_{i,:}\right)\mathrm{d}\latentVector_{i,:}p\left(\switchVector_{i,:}\right)\]

\end_inset


\begin_inset Formula \[
p\left(\dataVector|\paramVector\right)=\int\prod_{j=1}^{\dataDim}\gaussianDist{\dataScalar_{i,j}}{f_{j}\left(\latentVector_{i,:};\mappingMatrix,l\right)}{\dataStd^{2}}\sum_{\switchVector}\prod_{k=1}^{\numComponents}\delta\left(\latentVector_{i,:}-\hat{\latentVector}_{k,:}\right)^{\switchScalar_{i,k}}\pi_{k}^{\switchScalar_{i,k}}\mathrm{d}\latentVector_{i,:}\]

\end_inset


