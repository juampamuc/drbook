\end_layout

\begin_layout BeginFrame
\begin_inset ERT
status collapsed

\begin_layout Standard

[allowframebreaks]
\end_layout

\end_inset

Log Likelihood
\end_layout

\begin_layout Standard

\series bold
Sample approximation to log likelihood:
\end_layout

\begin_layout Standard
\begin_inset Formula \[
\log p\left(\dataMatrix|\paramVector\right)=\sum_{i=1}^{\numData}\log\frac{1}{\numComponents}\sum_{k=1}^{\numComponents}p\left(\dataVector_{i,:}|\paramVector,\bar{\hat{\latentVector}}_{k,:}\right)\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Note Note
status collapsed

\begin_layout Standard
\begin_inset Formula \[
\frac{\mathrm{d}}{\mathrm{d}\paramVector}\log p\left(\dataVector_{i,:}|\paramVector\right)=\sum_{k=1}^{\numComponents}\frac{1}{\sum_{m=1}^{\numComponents}p\left(\dataVector_{i,:}|\paramVector,\hat{\mathbf{x}}_{k,:}\right)}\frac{\mathrm{d}p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k}\right)}{\mathrm{d}\paramVector}\]

\end_inset

since we can relate
\begin_inset Formula \[
\frac{\mathrm{d}}{\mathrm{d}\paramVector}\log p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)=\frac{1}{p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)}\frac{\mathrm{d}p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)}{\mathrm{d}\paramVector}\]

\end_inset


\begin_inset Formula \[
\frac{\mathrm{d}p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)}{\mathrm{d}\paramVector}=p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)\frac{\mathrm{d}}{\mathrm{d}\paramVector}\log p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)\]

\end_inset


\end_layout

\end_inset

so we have
\size footnotesize

\begin_inset Formula \[
\frac{\mathrm{d}}{\mathrm{d}\paramVector}\log p\left(\dataVector_{i,:}|\paramVector\right)=\sum_{k=1}^{\numComponents}\frac{p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)}{\sum_{m=1}^{\numComponents}p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{m,:}\right)}\frac{\mathrm{d}}{\mathrm{d}\paramVector}\log p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)\]

\end_inset


\end_layout

\begin_layout Standard

\family roman
\size footnotesize
\lang british
\begin_inset Formula \[
\frac{\mathrm{d}}{\mathrm{d}\paramVector}\log p\left(\dataVector_{i,:}|\paramVector\right)=\sum_{k=1}^{\numComponents}\hat{\pi}_{i,k}\frac{\mathrm{d}}{\mathrm{d}\paramVector}\log p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Note Note
status collapsed

\begin_layout Standard
For
\begin_inset Formula \[
p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)=\prod_{j=1}^{\dataDim}\gaussianDist{y_{i,j}}{f_{j}\left(\hat{\latentVector}_{k,:};\paramVector\right)}{\dataStd^{2}}\]

\end_inset

we have
\begin_inset Formula \[
\frac{\mathrm{d}}{\mathrm{d}\paramVector}\log p\left(\dataVector_{i,:}|\paramVector,\hat{\latentVector}_{k,:}\right)=\frac{1}{\dataStd^{2}}\sum_{j=1}^{\dataDim}\left(\dataScalar_{i,j}-f_{j}\left(\hat{\latentVector}_{k,:};\paramVector\right)\right)\frac{\mathrm{d}f\left(\hat{\latentVector}_{k,:};\paramVector\right)}{\mathrm{d}\paramVector}\]

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard

\series bold
Note: 
\series default

\begin_inset Formula $\hat{\pi}{}_{i,k}$
\end_inset

 look a bit like the posterior over component 
\begin_inset Formula $k$
\end_inset

 for data point 
\begin_inset Formula $i$
\end_inset

.
